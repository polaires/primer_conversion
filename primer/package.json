{
  "name": "primers",
  "version": "1.0.0",
  "description": "PCR Primer Design Tool - JavaScript port of https://github.com/Lattice-Automation/primers",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots",
    "test:visual:report": "playwright show-report",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "immer": "^10.0.3",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "seqviz": "^3.10.10",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "xlsx": "^0.18.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lattice-Automation/primers"
  },
  "keywords": [
    "pcr",
    "primers",
    "dna",
    "molecular-biology",
    "gibson-assembly",
    "golden-gate"
  ],
  "license": "MIT"
}
